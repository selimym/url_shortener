# Production Environment Template
# Copy this file to .env.production and customize with your actual values
# NEVER commit .env.production with real credentials!

ENV_NAME="Production"
BASE_URL="https://your-domain.com"
DB_URL="postgresql+asyncpg://urlshortener:YOUR_SECURE_PASSWORD_HERE@postgres:5432/urlshortener"
REDIS_URL="redis://redis:6379/0"
USE_MIGRATIONS="true"
RATE_LIMIT_ENABLED="true"
RATE_LIMIT_CREATE=10
RATE_LIMIT_READ=100
